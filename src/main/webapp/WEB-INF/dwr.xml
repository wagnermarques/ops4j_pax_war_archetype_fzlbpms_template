<dwr>

  <!-- init is only needed if you are extending DWR 
  <init>
    <creator id="..." class="..."/>
    <converter id="..." class="..."/>
  </init>
	-->
  
  <!-- without allow, DWR isn't allowed to do anything
  <allow>
    <filter class="..."/>
    <create creator="..." javascript="..."/>
    <convert converter="..." match="..."/>
  </allow>
 
 -->
